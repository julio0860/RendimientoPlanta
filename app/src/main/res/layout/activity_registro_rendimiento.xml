<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context="com.adr.rendimientoplanta.RegistroRendimiento">

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="01/11/2016"
        android:id="@+id/textView39"
        android:layout_row="2"
        android:layout_column="20"
        android:layout_alignParentTop="true"
        android:layout_alignParentLeft="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PLANTA DON CARLOS"
        android:id="@+id/textView40"
        android:layout_row="2"
        android:layout_column="20"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView39"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="REG: "
        android:id="@+id/textView41"
        android:layout_row="2"
        android:layout_column="20"
        android:layout_alignParentTop="true"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:id="@+id/textView42"
        android:layout_row="2"
        android:layout_column="20"
        android:layout_alignBaseline="@+id/textView41"
        android:layout_alignBottom="@+id/textView41"
        android:layout_toRightOf="@+id/textView41"
        android:layout_toEndOf="@+id/textView41" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LADO: "
        android:id="@+id/textView43"
        android:layout_row="2"
        android:layout_column="20"
        android:layout_below="@+id/textView47"
        android:layout_alignLeft="@+id/textView47"
        android:layout_alignStart="@+id/textView47" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="A"
        android:id="@+id/textView44"
        android:layout_row="2"
        android:layout_column="20"
        android:layout_alignBaseline="@+id/textView43"
        android:layout_alignBottom="@+id/textView43"
        android:layout_toRightOf="@+id/textView43"
        android:layout_toEndOf="@+id/textView43" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="EST: "
        android:id="@+id/textView45"
        android:layout_row="2"
        android:layout_column="20"
        android:layout_alignBaseline="@+id/textView40"
        android:layout_alignBottom="@+id/textView40"
        android:layout_toLeftOf="@+id/textView42"
        android:layout_toStartOf="@+id/textView42" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="1"
        android:id="@+id/textView46"
        android:layout_row="2"
        android:layout_column="20"
        android:layout_alignBaseline="@+id/textView45"
        android:layout_alignBottom="@+id/textView45"
        android:layout_alignLeft="@+id/textView42"
        android:layout_alignStart="@+id/textView42" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="LINEA 1 - PUN PDC"
        android:id="@+id/textView47"
        android:layout_row="2"
        android:layout_column="20"
        android:layout_above="@+id/textView40"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DNI"
        android:id="@+id/lblDNI"
        android:layout_row="2"
        android:layout_column="20"
        android:layout_marginTop="24dp"
        android:layout_below="@+id/textView40"
        android:layout_alignStart="@+id/spnPresentacion" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="NOMBRES"
        android:id="@+id/lblNombres"
        android:layout_row="2"
        android:layout_column="20"
        android:layout_above="@+id/textView48"
        android:layout_toStartOf="@+id/textView45" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PRESENTACION"
        android:id="@+id/textView48"
        android:layout_row="2"
        android:layout_column="20"
        android:layout_marginTop="26dp"
        android:layout_below="@+id/lblDNI"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CANTIDAD"
        android:id="@+id/textView49"
        android:layout_row="2"
        android:layout_column="20"
        android:layout_below="@+id/spnPresentacion"
        android:layout_alignParentStart="true" />

    <RadioGroup
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_alignTop="@+id/button4"
        android:layout_toLeftOf="@+id/textView41"
        android:layout_toStartOf="@+id/textView41">

    </RadioGroup>

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberSigned"
        android:ems="10"
        android:id="@+id/editText4"
        android:layout_below="@+id/textView49"
        android:layout_toStartOf="@+id/spnPresentacion" />

    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/spnPresentacion"
        android:layout_alignBaseline="@+id/textView48"
        android:layout_alignBottom="@+id/textView48"
        android:layout_alignParentEnd="true"
        android:layout_toEndOf="@+id/textView48" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="REGISTRAR"
        android:id="@+id/button4"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/textView47" />

    <RadioButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="ENTREGA"
        android:id="@+id/radioButton"
        android:checked="true"
        android:layout_below="@+id/editText4"
        android:layout_alignParentStart="true"
        android:layout_marginTop="24dp" />

    <RadioButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="DEVOLUCION"
        android:id="@+id/radioButton2"
        android:checked="false"
        android:layout_alignTop="@+id/radioButton"
        android:layout_toEndOf="@+id/editText4" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="PESO"
        android:id="@+id/textView70"
        android:layout_row="2"
        android:layout_column="20"
        android:layout_below="@+id/spnPresentacion"
        android:layout_toEndOf="@+id/textView40" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="numberSigned"
        android:ems="10"
        android:id="@+id/editText5"
        android:layout_marginRight="24dp"
        android:layout_below="@+id/textView70"
        android:layout_alignStart="@+id/textView70"
        android:layout_toStartOf="@+id/textView45" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="MESA"
        android:id="@+id/textView73"
        android:layout_row="2"
        android:layout_column="20"
        android:layout_above="@+id/textView48"
        android:layout_alignParentStart="true" />
</RelativeLayout>
