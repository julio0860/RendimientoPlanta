<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".RendArmado_Kardex">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/lblFecha"
        android:layout_row="3"
        android:layout_column="7"
        android:enabled="false"
        android:inputType="date"
        android:layout_columnSpan="4"
        android:text="01/01/1900"
        android:textColor="#000000"
        android:focusableInTouchMode="false"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:layout_alignParentTop="true"
        android:layout_alignEnd="@+id/btnCambiarPresentacion" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="ADR"
        android:id="@+id/lblEmpresa"
        android:layout_alignTop="@+id/lblFecha"
        android:layout_alignParentStart="true"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="PLANTA DON CARLOS"
        android:id="@+id/lblSucursal"
        android:layout_alignParentTop="true"
        android:layout_alignStart="@+id/textView80"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="LINEA 1 PUN PDC"
        android:id="@+id/lblLinea"
        android:textStyle="bold"
        android:layout_below="@+id/imageView18"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="LADO A"
        android:id="@+id/lblLado"
        android:layout_alignTop="@+id/lblLinea"
        android:layout_alignStart="@+id/textView82"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="MESA"
        android:id="@+id/textView37"
        android:layout_below="@+id/textView38"
        android:layout_alignParentStart="true"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="@string/lbl_Detalle"
        android:id="@+id/textView38"
        android:layout_below="@+id/lblLinea"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="01"
        android:id="@+id/lblMesa"
        android:layout_below="@+id/textView37"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="DNI"
        android:id="@+id/textView80"
        android:layout_below="@+id/textView38"
        android:layout_alignStart="@+id/lblDNI"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="40084274"
        android:id="@+id/lblDNI"
        android:layout_below="@+id/textView80"
        android:layout_alignEnd="@+id/lblLinea" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="NOMBRES"
        android:id="@+id/textView82"
        android:layout_alignTop="@+id/textView80"
        android:layout_centerHorizontal="true"
        android:textStyle="bold" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="ERICKA NOEMI LABIOS SAYRITUPAC"
        android:id="@+id/lblNombres"
        android:layout_alignTop="@+id/lblDNI"
        android:layout_alignEnd="@+id/lblFecha"
        android:layout_toEndOf="@+id/lblDNI"
        android:layout_alignStart="@+id/textView82" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CAMBIAR PRESENTACION"
        android:id="@+id/btnCambiarPresentacion"
        android:layout_below="@+id/imageView24"
        android:layout_alignParentEnd="true" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imbRegresar"
        android:layout_row="0"
        android:layout_column="6"
        android:src="@android:drawable/ic_menu_revert"
        android:layout_alignParentBottom="true"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="DETALLE INGRESO"
        android:id="@+id/textView21"
        android:singleLine="false"
        android:textColor="#ffffff"
        android:background="#000000"
        android:layout_below="@+id/btnSincronizar"
        android:layout_alignParentStart="true"
        android:layout_marginTop="30dp" />

    <GridView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/lstIngresos"
        android:background="@color/common_action_bar_splitter"
        android:layout_above="@+id/imbRegresar"
        android:layout_alignEnd="@+id/lblNombres"
        android:layout_below="@+id/textView21" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:id="@+id/imageView18"
        android:background="#000000"
        android:baselineAlignBottom="false"
        android:layout_below="@+id/lblEmpresa"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:id="@+id/imageView19"
        android:background="#000000"
        android:baselineAlignBottom="false"
        android:layout_alignParentStart="true"
        android:layout_toStartOf="@+id/lblFecha"
        android:layout_below="@+id/lblLinea" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:id="@+id/imageView20"
        android:background="#000000"
        android:baselineAlignBottom="false"
        android:layout_below="@+id/textView38"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:id="@+id/imageView21"
        android:background="#000000"
        android:baselineAlignBottom="false"
        android:layout_below="@+id/textView37"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:id="@+id/imageView23"
        android:background="#000000"
        android:baselineAlignBottom="false"
        android:layout_below="@+id/textView21"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:id="@+id/imageView24"
        android:background="#000000"
        android:baselineAlignBottom="false"
        android:layout_alignTop="@+id/imbRegresar"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="REGISTRO"
        android:id="@+id/textView2"
        android:singleLine="false"
        android:textColor="#ffffff"
        android:background="#000000"
        android:layout_below="@+id/lblNombres"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="500G K36 INF"
        android:id="@+id/lblPresentacion"
        android:textStyle="bold"
        android:layout_alignTop="@+id/lblLado"
        android:layout_alignParentEnd="true"
        android:textColor="#ffffff"
        android:background="#000000"
        android:layout_above="@+id/imageView20"
        android:layout_alignStart="@+id/lblFecha"
        android:textAlignment="center" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:id="@+id/imageView25"
        android:background="#000000"
        android:baselineAlignBottom="false"
        android:layout_below="@+id/textView2"
        android:layout_alignParentStart="true" />

    <RadioGroup
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:orientation="vertical"
        android:id="@+id/radioGroup3"
        android:layout_below="@+id/imageView25"
        android:layout_alignParentStart="true"
        android:layout_alignEnd="@+id/lblDNI"
        android:layout_alignBottom="@+id/edtPeso">

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="ENTREGA"
            android:id="@+id/rbnEntrega"
            android:textAppearance="?android:attr/textAppearanceLarge"
            android:checked="true" />

        <RadioButton
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="DEVOLUCION"
            android:id="@+id/rbnDevolucion"
            android:textAppearance="?android:attr/textAppearanceLarge" />
    </RadioGroup>

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="CANTIDAD"
        android:id="@+id/textView3"
        android:singleLine="false"
        android:textColor="#ffffff"
        android:background="#000000"
        android:layout_alignTop="@+id/imageView25"
        android:layout_alignStart="@+id/lblNombres" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="PESO"
        android:id="@+id/textView22"
        android:singleLine="false"
        android:textColor="#ffffff"
        android:background="#000000"
        android:layout_below="@+id/textView3"
        android:layout_alignStart="@+id/textView3"
        android:layout_marginTop="22dp"
        android:layout_toStartOf="@+id/edtCantidad"
        android:layout_alignEnd="@+id/textView3" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/edtCantidad"
        android:layout_alignTop="@+id/imageView25"
        android:layout_toEndOf="@+id/textView82" />

    <EditText
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:inputType="number"
        android:ems="10"
        android:id="@+id/edtPeso"
        android:layout_alignTop="@+id/textView22"
        android:layout_alignStart="@+id/edtCantidad" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:id="@+id/imageView26"
        android:background="#000000"
        android:baselineAlignBottom="false"
        android:layout_below="@+id/edtCantidad"
        android:layout_alignStart="@+id/textView22" />

    <Button
        android:id="@+id/btnSincronizar"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:text="Agregar"
        android:layout_row="0"
        android:layout_column="3"
        android:layout_below="@+id/edtPeso"
        android:layout_alignParentStart="true" />

    <ImageView
        android:layout_width="match_parent"
        android:layout_height="2dp"
        android:id="@+id/imageView27"
        android:background="#000000"
        android:baselineAlignBottom="false"
        android:layout_below="@+id/radioGroup3"
        android:layout_alignParentStart="true" />


</RelativeLayout>
