<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:paddingBottom="@dimen/activity_vertical_margin"
    android:paddingLeft="@dimen/activity_horizontal_margin"
    android:paddingRight="@dimen/activity_horizontal_margin"
    android:paddingTop="@dimen/activity_vertical_margin"
    tools:context=".RendArmado_Kardex">
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="SUCURSAL"
        android:id="@+id/textView22"
        android:layout_row="3"
        android:layout_column="0"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="32dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="822dp" />

    <Spinner
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/spinner6"
        android:layout_row="3"
        android:layout_column="1"
        android:layout_alignParentLeft="true"
        android:layout_marginLeft="274dp"
        android:layout_alignParentTop="true"
        android:layout_marginTop="826dp" />

    <EditText
        android:layout_width="120dp"
        android:layout_height="wrap_content"
        android:ems="10"
        android:id="@+id/edtFecha"
        android:layout_row="3"
        android:layout_column="7"
        android:enabled="false"
        android:inputType="date"
        android:layout_columnSpan="4"
        android:text="01/01/1900"
        android:textColor="#000000"
        android:focusableInTouchMode="false"
        android:layout_alignParentTop="true"
        android:layout_alignEnd="@+id/spinner6" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="ADR"
        android:id="@+id/lblEmpresa"
        android:layout_alignTop="@+id/edtFecha"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="PLANTA DON CARLOS"
        android:id="@+id/lblSucursal"
        android:layout_alignTop="@+id/edtFecha"
        android:layout_centerHorizontal="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="LINEA 1 PUN PDC"
        android:id="@+id/lblLinea"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/lblSucursal"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="LADO A"
        android:id="@+id/lblLado"
        android:layout_below="@+id/lblSucursal"
        android:layout_alignStart="@+id/spinner6" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="Mesa"
        android:id="@+id/textView37"
        android:layout_below="@+id/textView38"
        android:layout_alignParentStart="true"
        android:layout_marginTop="16dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Detalle"
        android:id="@+id/textView38"
        android:layout_below="@+id/lblLinea"
        android:layout_alignParentStart="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="01"
        android:id="@+id/lblMesa"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/textView37"
        android:layout_alignParentLeft="true" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="DNI"
        android:id="@+id/textView80"
        android:layout_above="@+id/lblMesa"
        android:layout_alignStart="@+id/lblDNI" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="40084274"
        android:id="@+id/lblDNI"
        android:layout_below="@+id/edtFecha"
        android:layout_alignEnd="@+id/lblLinea"
        android:layout_marginTop="76dp" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceLarge"
        android:text="NOMBRES"
        android:id="@+id/textView82"
        android:layout_above="@+id/lblMesa"
        android:layout_centerHorizontal="true" />

    <GridView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/gridView"
        tools:listitem="@android:layout/simple_list_item_2"
        android:layout_above="@+id/button5"
        android:background="@color/common_action_bar_splitter"
        android:layout_below="@+id/textView88" />

    <Button
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:text="CERRAR DIA"
        android:id="@+id/button5"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/gridView" />

    <ImageButton
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:id="@+id/imbRegresar"
        android:layout_row="0"
        android:layout_column="6"
        android:src="@android:drawable/ic_menu_revert"
        android:layout_alignParentBottom="true"
        android:layout_alignEnd="@+id/textView37" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="Kardex"
        android:id="@+id/textView88"
        android:layout_alignParentStart="true"
        android:layout_below="@+id/lblMesa" />

    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:textAppearance="?android:attr/textAppearanceMedium"
        android:text="ERICKA NOEMI LABIOS SAYRITUPAC"
        android:id="@+id/textView24"
        android:layout_below="@+id/textView82"
        android:layout_alignStart="@+id/textView82" />

</RelativeLayout>
